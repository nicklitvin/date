generator client {
  provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url = env("POSTGRES_URL")
}

enum Gender {
    Male
    Female
}

enum SchoolYear {
    Freshman
    Sophomore
    Junior
    Senior
    Graduate
    Alumni
}

model User {
    id      String     @id
    email   String
    name    String
    age     Int
    gender  Gender
    interest Gender
    attributes String[]
    images  String[]
    description String
    notifications Boolean
    university String
}